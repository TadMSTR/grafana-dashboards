      "targets": [
        {
          "query": "import \"strings\"\r\nimport \"regexp\"\r\n\r\nfrom(bucket: \"unraid\")\r\n  |> range(start: -1h)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"disk\")\r\n  |> filter(fn: (r) => r[\"path\"] =~ /^\\/mnt\\/disk[0-9]+$/ or r[\"path\"] == \"/mnt/cache\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"used\" or r[\"_field\"] == \"total\" or r[\"_field\"] == \"used_percent\")\r\n  |> map(fn: (r) => ({ r with _value: float(v: r._value) }))\r\n  |> group(columns: [\"path\", \"_field\"])\r\n  |> last()\r\n  |> group()\r\n  |> pivot(rowKey: [\"path\"], columnKey: [\"_field\"], valueColumn: \"_value\")\r\n  |> map(fn: (r) => ({\r\n      r with \r\n      path: if regexp.matchRegexpString(r: /^\\/mnt\\/disk([0-9])$/, v: r.path) \r\n            then strings.replaceAll(v: r.path, t: \"/mnt/disk\", u: \"disk0\")\r\n            else strings.replaceAll(v: r.path, t: \"/mnt/\", u: \"\")\r\n    }))\r\n  |> sort(columns: [\"path\"])",
          "refId": "A"
        }
      ],
      "title": "Per-Disk Usage",
      "type": "table"
