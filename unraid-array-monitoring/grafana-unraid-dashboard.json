          "query": "import \"date\"\r\nimport \"experimental/array\"\r\n\r\n// Get total growth this month\r\nmonthGrowth = from(bucket: \"unraid\")\r\n  |> range(start: date.truncate(t: now(), unit: 1mo))\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"disk\")\r\n  |> filter(fn: (r) => r[\"path\"] == \"/mnt/user\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"used\")\r\n  |> difference(nonNegative: false, columns: [\"_value\"])\r\n  |> sum()\r\n  |> findRecord(fn: (key) => true, idx: 0)\r\n\r\n// Get total capacity\r\ntotal = from(bucket: \"unraid\")\r\n  |> range(start: -1h)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"disk\")\r\n  |> filter(fn: (r) => r[\"path\"] == \"/mnt/user\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"total\")\r\n  |> last()\r\n  |> findRecord(fn: (key) => true, idx: 0)\r\n\r\n// Get current usage\r\ncurrent = from(bucket: \"unraid\")\r\n  |> range(start: -1h)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"disk\")\r\n  |> filter(fn: (r) => r[\"path\"] == \"/mnt/user\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"used\")\r\n  |> last()\r\n  |> findRecord(fn: (key) => true, idx: 0)\r\n\r\n// Calculate days since Feb 1\r\nmonthStart = date.truncate(t: now(), unit: 1mo)\r\ndaysSinceStart = float(v: uint(v: now()) - uint(v: monthStart)) / 86400000000000.0\r\n\r\n// Daily growth rate (convert to float)\r\ngrowthPerDay = float(v: monthGrowth._value) / daysSinceStart\r\n\r\n// Remaining space (convert to float)\r\nremaining = float(v: total._value) - float(v: current._value)\r\n\r\n// Days until full\r\ndaysUntilFull = if growthPerDay > 0.0 then remaining / growthPerDay else 999999.0\r\n\r\n// Return result\r\narray.from(rows: [{_time: now(), _value: daysUntilFull}])",
