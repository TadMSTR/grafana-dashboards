      "targets": [
        {
          "query": "import \"strings\"\r\n\r\nfrom(bucket: \"unraid\")\r\n  |> range(start: -5m)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"diskio\")\r\n  |> filter(fn: (r) => r[\"name\"] =~ /^sd[b-u]$/ or r[\"name\"] =~ /^nvme[1-2]n1$/)\r\n  |> filter(fn: (r) => r[\"_field\"] == \"read_bytes\" or r[\"_field\"] == \"write_bytes\")\r\n  |> derivative(unit: 1s, nonNegative: true)\r\n  |> map(fn: (r) => ({\r\n      r with \r\n      name: if r.name == \"nvme2n1\" or r.name == \"nvme1n1\" then \"cache\"\r\n            else if r.name == \"sdb\" then \"parity1\"\r\n            else if r.name == \"sdc\" then \"parity2\"\r\n            else if r.name == \"sdd\" then \"disk01\"\r\n            else if r.name == \"sde\" then \"disk02\"\r\n            else if r.name == \"sdf\" then \"disk03\"\r\n            else if r.name == \"sdg\" then \"disk04\"\r\n            else if r.name == \"sdh\" then \"disk05\"\r\n            else if r.name == \"sdi\" then \"disk06\"\r\n            else if r.name == \"sdj\" then \"disk07\"\r\n            else if r.name == \"sdk\" then \"disk08\"\r\n            else if r.name == \"sdl\" then \"disk09\"\r\n            else if r.name == \"sdm\" then \"disk10\"\r\n            else if r.name == \"sdn\" then \"disk11\"\r\n            else if r.name == \"sdo\" then \"disk12\"\r\n            else if r.name == \"sdp\" then \"disk13\"\r\n            else if r.name == \"sdq\" then \"disk14\"\r\n            else if r.name == \"sdr\" then \"disk15\"\r\n            else if r.name == \"sds\" then \"disk16\"\r\n            else if r.name == \"sdt\" then \"disk17\"\r\n            else if r.name == \"sdu\" then \"disk18\"\r\n            else r.name\r\n    }))\r\n  |> group(columns: [\"name\", \"_field\"])\r\n  |> sum()\r\n  |> group()\r\n  |> pivot(rowKey: [\"name\"], columnKey: [\"_field\"], valueColumn: \"_value\")\r\n  |> sort(columns: [\"name\"])",
          "refId": "A"
        }
      ],
      "title": "Per-Disk I/O",
      "type": "table"
